#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char* main(int argc, char *str)
{

char temp;
size_t len = strlen(str) -1;
size_t i;
size_t k = len;

	if (argc == 2) {

	for(i=0; i<len; i++)
		{
			temp = str[k];
			str[k] = str[i];
			str[i] = temp;
			k--;

		printf("%s\n",str);

		}
	}

	else{
		printf("print2");
		exit(0);

	}
}


